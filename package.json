{
  "name": "electron-router-dom",
  "private": true,
  "scripts": {
    "build": "turbo --filter=!./examples/* build",
    "build:package": "turbo --filter=./packages/electron-router-dom build",
    "dev": "turbo --filter=!./examples/* dev",
    "start": "turbo --filter=!./examples/* start",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/minimatch": "^6.0.0",
    "husky": "^9.1.4",
    "prettier": "^3.3.3",
    "turbo": "^2.8.1",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "next": "16.1.6"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^19.2.10",
      "@types/react-dom": "^19.2.3",
      "electron-router-dom>@types/react": "^18.0.20",
      "electron-router-dom>@types/react-dom": "^18.0.10",
      "tailwindcss": "^3.4.10"
    }
  }
}
