{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "module": "ESNext",
    "declaration": false,
    "declarationMap": false,
    "moduleResolution": "node",
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true
  }
}